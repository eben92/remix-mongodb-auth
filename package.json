{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -i ./app/styles/tailwind.css -o ./app/tailwind.css",
    "dev:run-all": "concurrently \"npm run generate:css -- --watch\" \"remix dev\" \"npm run sass\"",
    "sass": "sass --watch app/sass/:sass/",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@remix-run/node": "^1.8.2",
    "@remix-run/react": "^1.8.2",
    "@remix-run/vercel": "^1.8.2",
    "@vercel/node": "^2.6.2",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.56.2",
    "tiny-invariant": "^1.3.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.8.2",
    "@remix-run/eslint-config": "^1.8.2",
    "@remix-run/serve": "^1.8.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/validator": "^13.7.10",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "postcss": "^8.4.19",
    "prisma": "^4.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
